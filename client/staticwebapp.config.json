{
  "trailingSlash": "auto",
  "routes": [
    {
      "route": "/api/*",
      "methods": [
        "GET"
      ],
      "allowedRoles": [
        "authenticated"
      ]
    },
    {
      "route": "/api/*",
      "methods": [
        "PUT",
        "POST",
        "PATCH",
        "DELETE"
      ],
      "allowedRoles": [
        "contributor"
      ]
    },
    {
      "route": "/login",
      "rewrite": "/.auth/login/google"
    },
    {
      "route": "/logout",
      "redirect": "/.auth/logout"
    }
  ],
  "auth": {
    "rolesSource": "/api/getRoles",
    "identityProviders": {
      "github": {
        "clientIdSettingName": "GITHUB_CLIENT_ID",
        "clientSecretSettingName": "GITHUB_CLIENT_SECRET"
      }
    }
  }
}